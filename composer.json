{
  "name": "enjoyscms/admin",
  "description": "Module Admin for EnjoysCMS",
  "type": "enjoyscms-module",
  "require": {
    "php": "^7.4 | ^8.0",
    "oomphinc/composer-installers-extender": "^2.0",
    "enjoyscms/summernote": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "App\\Module\\Admin\\": "src/"
    }
  },
  "extra": {
    "module-name": "Admin panel",
    "use-migrations": false
  },
  "scripts": {
    "post-install-cmd": "cd ./modules/admin && yarn install",
    "post-update-cmd": "@post-install-cmd"
  }
}
